{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@jspaste/frontend",
	"private": true,
	"license": "EUPL-1.2",
	"type": "module",
	"scripts": {
		"build": "bun run build:server",
		"build:debug": "rm -rf ./dist/ && NODE_ENV=development bun vite build",
		"build:server": "rm -rf ./dist/ && bun vite build && bun run script:server && bun run script:compress",
		"clean:git:all": "bun run clean:git:untracked && bun run clean:git:gc && bun run clean:git:hooks",
		"clean:git:all:force": "bun run clean:git:untracked:force && bun run clean:git:gc && bun run clean:git:hooks",
		"clean:git:gc": "git gc --aggressive --prune",
		"clean:git:hooks": "rm -rf ./.git/hooks/ && bun install -f",
		"clean:git:untracked": "git clean -d -x -i",
		"clean:git:untracked:force": "git clean -d -x -f",
		"dev": "bun run start:dev",
		"fix": "bun run fix:biome; bun run fix:package",
		"fix:biome": "bun biome check --write",
		"fix:package": "bun sort-package-json --quiet",
		"lint": "bun run lint:biome && bun run lint:tsc",
		"lint:biome": "bun biome lint",
		"lint:tsc": "bun tsc --noEmit",
		"script:compress": "bun run ./scripts/compress.ts",
		"script:server": "bun run ./scripts/server.ts",
		"start": "bun run start:server",
		"start:dev": "bun vite",
		"start:rebuild": "bun run build:server && bun run start:server",
		"start:server": "NODE_ENV=production bun run --cwd=./dist/ ./index.js"
	},
	"dependencies": {
		"@codemirror/autocomplete": "^6.18.0",
		"@codemirror/commands": "^6.8.0",
		"@codemirror/lang-cpp": "^6.0.0",
		"@codemirror/lang-css": "^6.3.0",
		"@codemirror/lang-go": "^6.0.0",
		"@codemirror/lang-html": "^6.4.0",
		"@codemirror/lang-java": "^6.0.0",
		"@codemirror/lang-javascript": "^6.2.0",
		"@codemirror/lang-json": "^6.0.0",
		"@codemirror/lang-markdown": "^6.3.0",
		"@codemirror/lang-php": "^6.0.0",
		"@codemirror/lang-python": "^6.1.0",
		"@codemirror/lang-rust": "^6.0.0",
		"@codemirror/lang-xml": "^6.1.0",
		"@codemirror/lang-yaml": "^6.1.0",
		"@codemirror/language": "^6.10.0",
		"@codemirror/state": "^6.5.0",
		"@codemirror/view": "^6.36.0",
		"@solid-primitives/media": "~2.3.0",
		"@solid-primitives/scheduled": "~1.5.0",
		"@solid-primitives/storage": "~4.3.0",
		"@solidjs/router": "~0.15.0",
		"@tabler/icons-solidjs": "^3.28.0",
		"@uiw/codemirror-extensions-hyper-link": "^4.23.0",
		"@uiw/codemirror-themes": "^4.23.0",
		"daisyui": "5.0.0-beta.6",
		"env-var": "~7.5.0",
		"solid-js": "~1.9.0"
	},
	"devDependencies": {
		"@biomejs/biome": "~1.9.0",
		"@tailwindcss/vite": "4.0.0",
		"@types/bun": "^1.1.0",
		"browserslist": "^4.24.0",
		"lefthook": "~1.10.0",
		"lightningcss": "^1.29.0",
		"rollup-plugin-visualizer": "~5.14.0",
		"sort-package-json": "~2.14.0",
		"vite": "~6.0.0",
		"vite-plugin-solid": "~2.11.0"
	},
	"peerDependencies": {
		"typescript": "~5.5.0"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"lefthook"
	]
}
